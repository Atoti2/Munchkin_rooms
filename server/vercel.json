{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/munchkin/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/munchkin/dist" 
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/munchkin/dist/$1" 
    }
  ]
}
